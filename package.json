{
  "name": "assessment-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest --config ./vite.config.ts",
    "build:staging": "tsc && vite build --mode staging",
    "build:qa": "tsc && vite build --mode qa",
    "build:wong": "tsc && vite build --mode wong",
    "deploy:staging": "npm run build:staging && aws s3 sync ./dist s3://sg.assessment-staging.supply25.com --profile",
    "deploy:qa": "npm run build:qa && aws s3 sync ./dist s3://sg.assessment-qa.supply25.com --profile",
    "deploy:wong": "npm run build:wong && aws s3 sync ./dist s3://assessment-app-wong.supply25.com --profile"
  },
  "dependencies": {
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@mui/material": "^5.16.6",
    "@tanstack/react-query": "^5.59.19",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.0",
    "styled-components": "^6.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@scottish-government/design-system": "^2.2.0",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@types/node": "^22.3.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^9.8.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "jsdom": "^24.1.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.0",
    "vite": "^5.4.0",
    "vitest": "^2.0.5"
  }
}
